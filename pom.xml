            <unsubscribe>private-unsubscribe@tamaya.incubator.apache.org</unsubscribe>
            <post>private@tamaya.incubator.apache.org</post>
            <archive>http://mail-archives.apache.org/mod_mbox/tamaya-private/</archive>
        </mailingList>
